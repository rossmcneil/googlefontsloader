{
    "name": "rossmcneil/googlefontsloader",
    "description": ":description",
    "license": "MIT",
    "authors": [
        {
            "name": "Ross McNeil",
            "email": "ross@digizu.co.uk",
            "homepage": "https://rossmcneil.com"
        }
    ],
    "homepage": "https://github.com/rossmcneil/googlefontsloader",
    "keywords": ["Laravel", "GoogleFontsLoader"],
    "require": {
        "illuminate/support": "~5|~6"
    },
    "require-dev": {
        "phpunit/phpunit": "~7.0",
        "mockery/mockery": "^1.1",
        "orchestra/testbench": "~3.0",
        "sempro/phpunit-pretty-print": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "RossMcNeil\\GoogleFontsLoader\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "RossMcNeil\\GoogleFontsLoader\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "RossMcNeil\\GoogleFontsLoader\\GoogleFontsLoaderServiceProvider"
            ],
            "aliases": {
                "GoogleFontsLoader": "RossMcNeil\\GoogleFontsLoader\\Facades\\GoogleFontsLoader"
            }
        }
    }
}
